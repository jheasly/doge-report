  "paths": {
    "/savings/grants": {
      "get": {
        "tags": [
          "Savings"
        ],
        "summary": "Get grant savings",
        "operationId": "get_GrantSavings",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "enum": [
                "savings",
                "value",
                "date"
              ],
              "description": "The field to sort by"
            },
            "required": false,
            "description": "The field to sort by",
            "name": "sort_by",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "enum": [
                "asc",
                "desc"
              ],
              "description": "The order to sort by"
            },
            "required": false,
            "description": "The order to sort by",
            "name": "sort_order",
            "in": "query"
          },
          {
            "schema": {
              "type": "number",
              "minimum": 1,
              "default": 1,
              "description": "The page number to check"
            },
            "required": false,
            "description": "The page number to check",
            "name": "page",
            "in": "query"
          },
          {
            "schema": {
              "type": "number",
              "maximum": 500,
              "minimum": 1,
              "default": 100,
              "description": "The number of items per page from 1 to 500"
            },
            "required": false,
            "description": "The number of items per page from 1 to 500",
            "name": "per_page",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Returns the grant savings",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "result": {
                      "type": "object",
                      "properties": {
                        "grants": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "date": {
                                "type": "string",
                                "description": "The cancellation date of the grant",
                                "example": "2/7/2025"
                              },
                              "agency": {
                                "type": "string",
                                "description": "The agency of the grant",
                                "example": "Department Of Education"
                              },
                              "recipient": {
                                "type": "string",
                                "description": "The recipient of the grant",
                                "example": "Virginia Commonwealth University"
                              },
                              "value": {
                                "type": "number",
                                "description": "The dollar value of the grant",
                                "example": "9475685"
                              },
                              "savings": {
                                "type": "number",
                                "description": "The savings on the grant",
                                "example": "8463038"
                              },
                              "link": {
                                "type": [
                                  "string",
                                  "null"
                                ],
                                "description": "The link to the grant on usaspending.gov",
                                "example": "https://www.usaspending.gov/award/1234567890"
                              },
                              "description": {
                                "type": [
                                  "string",
                                  "null"
                                ],
                                "description": "The description of the grant",
                                "example": "This grant is for the construction of a new building on the campus of Virginia Commonwealth University."
                              }
                            },
                            "required": [
                              "date",
                              "agency",
                              "recipient",
                              "value",
                              "savings",
                              "link",
                              "description"
                            ]
                          }
                        }
                      },
                      "required": [
                        "grants"
                      ]
                    },
                    "meta": {
                      "type": "object",
                      "properties": {
                        "total_results": {
                          "type": "number",
                          "description": "The total amount of results"
                        },
                        "pages": {
                          "type": "number",
                          "description": "The total amount of pages at current per_page limit"
                        }
                      },
                      "required": [
                        "total_results",
                        "pages"
                      ]
                    }
                  },
                  "required": [
                    "success",
                    "result",
                    "meta"
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Schema validation error. Please check the documentation and try again.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": "false"
                    },
                    "message": {
                      "type": "string",
                      "example": "Schema validation error. Please check the documentation and try again."
                    }
                  },
                  "required": [
                    "success",
                    "message"
                  ]
                }
              }
            }
          },
          "429": {
            "description": "Rate limit exceeded. Please try again later.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": "false"
                    },
                    "message": {
                      "type": "string",
                      "example": "Rate limit exceeded. Please try again later."
                    }
                  },
                  "required": [
                    "success",
                    "message"
                  ]
                }
              }
            }
          },
          "500": {
            "description": "An error occurred while processing the request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": "false"
                    },
                    "message": {
                      "type": "string",
                      "example": "An error occurred while processing your request. We're on it."
                    }
                  },
                  "required": [
                    "success",
                    "message"
                  ]
                }
              }
            }
          }
        }
      }
    },
